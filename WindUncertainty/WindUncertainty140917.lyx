\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{eqnarray}
\left\langle (\delta w)^{2}\right\rangle  & = & V^{2}\left(\left\langle (\delta\alpha)^{2}\right\rangle -2\left\langle \delta\alpha\delta\phi\right\rangle +\left\langle (\delta\phi)^{2}\right\rangle \right)\label{eq:delta-w-sq-simplified}
